{
    "name": "Paranoia",
    "description": "@paranoia_v1",
    "version": "0.1",
    "permissions": [
        "tabs","<all_urls>",    "activeTab"

    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
        "matches": [
            "https://opensea.io/*"
            ],
        "js": ["content.js"],
		"all_frames": true,
        "run_at": "document_idle"
        }
    ], 
    "manifest_version": 2
}