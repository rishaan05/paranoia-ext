{
    "name": "Paranoia",
    "description": "@paranoia_v1",
    "version": "0.1",
    "permissions": [
        "tabs",
        "<all_urls>",
        "activeTab",
        "storage"
    ],
    "icons": {
        "16": "icons/icon_16.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
    },
    "browser_action": {
        "default_icon": "icons/icon_128.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://opensea.io/*"
            ],
            "js": [
                "content.js"
            ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ],
    "manifest_version": 2
}